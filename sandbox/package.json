    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-native": "0.72.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@tsconfig/react-native": "^3.0.2",
    "@types/jest": "^29.5.4",
    "babel-jest": "^29.7.0",
    "eslint": "^8.49.0",
    "eslint-plugin-import": "^2.28.1",
    "jest": "^29.7.0",
    "prettier": "^2.4.1",
    "typescript": "^5.2.2"
  }
